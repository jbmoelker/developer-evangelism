  "license": "MIT",
  "devDependencies": {
    "bluebird": "3.1.1",
    "commitizen": "2.5.0",
    "cz-conventional-changelog": "1.1.5",
    "favicons": "4.4.5",
    "http-server": "0.8.5",
    "mkdirp": "0.5.1",
    "nunjucks": "2.3.0",
    "x-ray": "2.0.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
